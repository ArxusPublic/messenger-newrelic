{
    "name": "arxus/messenger-newrelic",
    "description": "Make symfony messenger report consumption to newrelic",
    "license": "proprietary",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "symfony/messenger": "^4.3",
        "symfony/http-kernel": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16.1",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "nikic/php-parser": "^4.0",
        "phpro/grumphp": "^0.17",
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master",
        "symfony/process": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Arxus\\NewrelicMessengerBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arxus\\NewrelicMessengerBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
